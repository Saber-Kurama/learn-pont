{
  "name": "pont-engine",
  "version": "1.0.12",
  "description": "pont swagger typescript API",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": {
    "pont": "./bin/init.js"
  },
  "scripts": {
    "pretest": "npm run build",
    "dev": "tsc -w",
    "watch": "tsc -w",
    "build-hooks-js": "tsc --project pontCore/tsconfig.json",
    "build": "rm -rf lib && npm run build-hooks-js && tsc",
    "pont1": "pont"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^8.1.0",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.1.2",
    "lodash": "^4.17.21",
    "node-fetch": "^3.0.0",
    "prettier": "^2.3.2",
    "translation.js": "^0.7.15",
    "typescript": "^4.4.2"
  },
  "files": [
    "pontCore",
    "configSchema.json",
    "src",
    "lib",
    "*.md",
    "bin",
    "templates",
    "useRequest"
  ],
  "keywords": [
    "pont",
    "swagger",
    "typescript",
    "API"
  ],
  "author": "jasonHzq",
  "license": "MIT"
}
